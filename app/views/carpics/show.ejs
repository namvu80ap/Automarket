<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.carpics %>">carpics</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of carpic</a></li>
</ul>

<div class="page-header">
    <h1>Details of carpic</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>carId</th><td><%= carpic.carId %></td></tr>

        <tr><th>name</th><td><%= carpic.name %></td></tr>

        <tr><th>url</th><td><%= carpic.url %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_carpic(carpic), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.carpic(carpic), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.carpics) %>
</div>
