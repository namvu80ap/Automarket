<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.cars %>">cars</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of car</a></li>
</ul>

<div class="page-header">
    <h1>Details of car</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>name</th><td><%= car.name %></td></tr>

        <tr><th>description</th><td><%- car.description %></td></tr>

        <tr><th>maker</th><td><%= car.maker %></td></tr>

        <tr><th>model</th><td><%= car.model %></td></tr>

        <tr><th>price</th><td><%= car.price %></td></tr>

        <tr><th>proDate</th><td><%= car.proDate %></td></tr>

        <tr><th>leftRight</th><td><%= car.leftRight %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_car(car), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.car(car), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.cars) %>
</div>
