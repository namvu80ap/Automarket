<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.cars() %>">cars</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New car</a></li>
</ul>

<div class="page-header">
    <h1>New car</h1>
</div>

<% var form = formFor(car, {action: pathTo.cars(), method: 'POST', id: "car-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create car', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.cars(), {class: 'btn'}) %>
</div>
<%- form.end() %>
